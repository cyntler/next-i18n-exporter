{
  "name": "next-locales",
  "version": "1.0.0",
  "description": "Internationalization support for Next.js even with Static HTML export.",
  "keywords": [
    "i18n",
    "next",
    "nextjs",
    "html",
    "export"
  ],
  "main": "./dist/exports/index.js",
  "types": "./dist/exports/index.d.ts",
  "exports": {
    ".": "./dist/exports/index.js",
    "./server": "./dist/exports/server.js"
  },
  "typesVersions": {
    "*": {
      "server": [
        "./dist/exports/server.d.ts"
      ]
    }
  },
  "scripts": {
    "start": "tsc --watch",
    "build": "rimraf ./dist && tsc",
    "lint": "eslint ./src --ext ts,tsx",
    "prettier": "prettier --check ./src"
  },
  "author": "Damian Cyntler",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^17.0.14",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "find-project-root": "^1.1.1",
    "mustache": "^4.2.0",
    "next-language-detector": "^1.0.2",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@cyntler/eslint-config": "^0.0.1",
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/mustache": "^4.1.2",
    "eslint": "^8.9.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "next": ">=12.0.10",
    "react": ">=17.0.2"
  }
}
